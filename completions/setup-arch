_setup_arch ()
{
	local cur prev opts base
	COMPREPLY=()
	cur="${COMP_WORDS[COMP_CWORD]}"
	prev="${COMP_WORDS[COMP_CWORD-1]}"
	all_long_opts="--root-dir --dry-run --help "
	all_short_opts="-r -d -h "
	case "$prev" in
		--root-dir|-r)
			COMPREPLY=( $(compgen -o bashdefault -o default -- "${cur}") )
			return 0
			;;
		*)
			case "$cur" in
				--*)
					COMPREPLY=( $(compgen -W "${all_long_opts}" -- "${cur}") )
					return 0
					;;
				-*)
					COMPREPLY=( $(compgen -W "${all_short_opts}" -- "${cur}") )
					return 0
					;;
				*)
					COMPREPLY=( $(compgen -o bashdefault -o default -- "${cur}") )
					return 0
					;;
			esac
	esac

}
complete -F _setup_arch setup-arch
### END OF CODE GENERATED BY Argbash (sortof) ### ])
